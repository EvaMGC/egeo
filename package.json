{
  "name": "egeo",
  "version": "0.6.0-RC1",
  "description": "Egeo Library. Common components, styles, fonts and website",
  "author": "Stratio",
  "scripts": {
    "definition:generate": "tsc -p tsconfig.declaration.json",
    "clean": "npm run clean:components && npm run clean:doc && npm run clean:coverage",
    "clean:components": "rimraf lib",
    "clean:coverage": "rimraf target/coverage",
    "clean:doc": "rimraf target/site/web",
    "build": "npm run clean && npm run build:doc && npm run build:components",
    "build:doc": "cross-env NODE_ENV=production webpack --no-color --bail",
    "build:components": "webpack --config config/webpack-components.js  --no-color --bail && npm run definition:generate",
    "lint": "tslint -c ./tslint.json -e 'node_modules' --type-check -t stylish '{web,components}/**/*.ts'",
    "replace": "json -I -f package.json -e",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --profile --open --watch --content-base ./",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Stratio/egeo"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "2.4.4",
    "@angular/compiler": "2.4.4",
    "@angular/core": "2.4.4",
    "@angular/forms": "2.4.4",
    "@angular/http": "2.4.4",
    "@angular/platform-browser": "2.4.4",
    "@angular/platform-browser-dynamic": "2.4.4",
    "@angular/router": "3.4.4",
    "core-js": "2.4.1",
    "ng2-translate": "2.5.0",
    "rxjs": "5.0.3",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.41",
    "@types/lodash": "4.14.50",
    "@types/node": "7.0.0",
    "@types/prismjs": "1.4.18",
    "angular-router-loader": "0.5.0",
    "angular2-template-loader": "0.6.0",
    "assets-webpack-plugin": "3.5.0",
    "awesome-typescript-loader": "2.2.4",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "dts-generator": "2.0.0",
    "egeo-theme": "0.2.0",
    "egeo-ui-base": "0.4.0",
    "file-loader": "0.9.0",
    "flexboxgrid": "6.3.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.26.0",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine-core": "2.5.2",
    "json": "9.0.4",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.1",
    "lodash": "4.17.4",
    "markdown-to-html-pipe": "1.2.0",
    "node-sass": "4.3.0",
    "null-loader": "0.1.1",
    "prismjs": "1.6.0",
    "prismjs-loader": "0.0.4",
    "raw-loader": "0.5.1",
