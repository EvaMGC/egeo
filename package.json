{
   "name": "egeo",
   "version": "",
   "description": "Egeo Library. Common components, styles, fonts and website",
   "author": "Stratio",
   "scripts": {
      "definition:generate": "tsc -p tsconfig.declaration.json",
      "clean": "npm run clean:lib && npm run clean:doc && npm run clean:coverage",
      "clean:lib": "rimraf lib",
      "clean:coverage": "rimraf target/coverage",
      "clean:doc": "rimraf target/site/web",
      "build": "npm run clean && npm run webpack:doc && npm run webpack:lib",
      "build:doc": "npm run clean:doc && npm run webpack:doc",
      "build:lib": "npm run clean:lib && npm run webpack:lib",
      "webpack:doc": "cross-env NODE_ENV=production webpack --no-color --bail",
      "webpack:lib": "webpack --config config/webpack-components.js  --no-color --bail && npm run definition:generate",
      "lint": "tslint -c ./tslint.json -e 'node_modules' --type-check -t stylish '{web,components}/**/*.ts'",
      "replace": "json -I -f package.json -e",
      "start": "cross-env NODE_ENV=development webpack-dev-server --progress --profile --open --watch --content-base ./",
      "test": "cross-env NODE_ENV=test karma start",
      "test:watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run"
   },
   "main": "lib/egeo.js",
   "typings": "lib/index.d.ts",
   "repository": {
      "type": "git",
      "url": "https://github.com/Stratio/egeo"
   },
   "license": "Apache-2.0",
   "dependencies": {
      "@angular/common": "2.4.6",
      "@angular/compiler": "2.4.6",
