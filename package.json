{
  "name": "egeo",
  "version": "1.0.0-RC1",
  "description": "Egeo Library. Common components, styles, fonts and website",
  "author": "Stratio",
  "scripts": {
    "definition:generate": "tsc -p tsconfig.declaration.json",
    "clean": "npm run clean:lib && npm run clean:doc && npm run clean:coverage",
    "clean:lib": "rimraf lib",
    "clean:coverage": "rimraf target/coverage",
    "clean:doc": "rimraf target/site/web",
    "clean:dll": "npm run rimraf -- dll",
    "build": "npm run clean && npm run webpack:doc && npm run webpack:lib",
    "build:doc": "npm run clean:doc && npm run webpack:doc",
    "build:lib": "npm run clean:lib && npm run webpack:lib",
    "webpack:doc": "cross-env NODE_ENV=production webpack --no-color --bail",
    "webpack:lib": "webpack --config config/webpack-components.js  --no-color --bail && npm run definition:generate",
    "lint": "tslint -c ./tslint.json -e 'node_modules' --type-check -t stylish '{web,components}/**/*.ts'",
    "replace": "json -I -f package.json -e",
    "rimraf": "rimraf",
    "start:dev": "npm run start -- --hot --inline",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --profile --open --watch --content-base ./",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run"
  },
  "main": "lib/egeo.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stratio/egeo"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "3.4.7",
    "core-js": "2.4.1",
    "rxjs": "5.1.1",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.7",
    "@angularclass/hmr": "1.2.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.43",
    "@types/lodash": "4.14.52",
    "@types/node": "7.0.5",
    "@types/prismjs": "1.4.18",
    "add-asset-html-webpack-plugin": "1.0.2",
    "angular2-template-loader": "0.6.2",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "3.0.3",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "egeo-theme": "0.3.0",
    "egeo-ui-base": "1.0.0",
    "file-loader": "0.10.0",
    "flexboxgrid": "6.3.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "json": "9.0.4",
    "json-loader": "0.5.4",
    "karma": "1.4.1",
    "karma-coverage": "1.1.1",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "lodash": "4.17.4",
    "markdown-to-html-pipe": "1.2.0",
    "ng-router-loader": "2.1.0",
    "ng2-translate": "5.0.0",
    "ngc-webpack": "1.2.0",
    "node-sass": "4.5.0",
    "null-loader": "0.1.1",
    "prismjs": "1.6.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.0",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "tslint": "4.4.2",
    "tslint-loader": "3.4.2",
    "typescript": "2.1.6",
    "url-loader": "0.5.7",
    "v8-lazy-parse-webpack-plugin": "0.3.0",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "2.6.1"
  }
}
