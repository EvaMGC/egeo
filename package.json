    "@types/lodash": "4.14.37",
    "@types/node": "6.0.45",
    "@types/prismjs": "1.4.18",
    "angular2-template-loader": "0.5.0",
    "awesome-typescript-loader": "2.2.4",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.5.2",
    "json": "9.0.4",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
