{
  "name": "egeo",
  "version": "",
  "license": "Apache-2.0",
  "description": "Egeo Library. Common components, styles, fonts and website",
  "scripts": {
    "start": "npm run ui-fonts && webpack-dev-server --inline --quiet --port 8080",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "build": "rm -rf target/generated-web && webpack --config config-web/webpack.prod.js --no-color --bail",
    "replace": "json -I -f package.json -e",
    "sass-lint": "sass-lint -c .sass-lint.yml ui-base/**/*.scss -v -q",
    "check-ui-base": "node-sass --include-path scss ui-base/index.scss public/main.css",
    "ui-fonts": "node-sass --include-path scss ui-fonts/index.scss ui-fonts/dist/ui-fonts.css"
  },
  "author": "Stratio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stratio/egeo"
  },
  "dependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/router": "3.1.0",
    "core-js": "2.4.1",
    "flexboxgrid": "6.3.1",
    "ng2-translate": "2.5.0",
    "prismjs": "1.5.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/es6-shim": "0.31.32",
