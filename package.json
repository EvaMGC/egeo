{
  "name": "egeo.angular2.components",
  "version": "0.3.0-RC1",
  "description": "Egeo Angular 2 Components Library.",
  "scripts": {
    "start": "webpack-dev-server --inline --quiet --port 8080",
    "test": "karma start",
    "build": "rm -rf target/generated-web && webpack --config config/webpack.prod.js --no-color --bail",
    "build:components": "rm -rf target/components && webpack --config config-components/webpack.prod.js --no-color --bail",
    "build:components-dev": "rm -rf target/components && webpack --config config-components/webpack.dev.js --pogress --bail",
    "postinstall": "typings install",
    "replace": "json -I -f package.json -e"
  },
  "author": "Stratio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stratio/egeo.angular2.components"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "bootstrap": "3.3.7",
    "egeo.ui.base": "*",
    "core-js": "2.4.1",
    "ng2-translate": "2.4.4",
    "prismjs": "1.5.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.5.1",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "3.10.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.12",
    "prismjs-loader": "0.0.3",
    "raw-loader": "0.5.1",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.3.3",
    "ts-helpers": "1.1.1",
    "webpack": "1.13.2",
    "webpack-dashboard": "0.1.8",
    "webpack-dev-server": "1.15.1",
    "webpack-merge": "0.14.1",
    "json": "9.0.4"
  }
}
