{
   "atom": {
      "rewriteTsconfig": false
   },
   "compilerOptions": {
      "declaration": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "pretty": true,
      "removeComments": false,
      "sourceMap": true,
      "suppressImplicitAnyIndexErrors": true,
      "target": "es5"
   },
   "exclude": [
      "node_modules",
      "dist"
   ]
}
