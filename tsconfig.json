{
   "atom": {
      "rewriteTsconfig": false
   },
   "awesomeTypescriptLoaderOptions": {
      "forkChecker": true,
      "useWebpackText": true
   },
   "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./web",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noEmitHelpers": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "strictNullChecks": false,
      "target": "es5"
   },
   "exclude": [
      "node_modules",
      "dist"
   ]
}
